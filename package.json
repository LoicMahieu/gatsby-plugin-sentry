{
  "name": "@loicmahieu/gatsby-plugin-sentry",
  "description": "Gatsby plugin to add Sentry error tracking to your site.",
  "version": "1.2.0",
  "author": "Loic Mahieu <mahieuloic@gmail.com>",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "files": [
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loicmahieu/gatsby-plugin-sentry.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "sentry"
  ],
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "@sentry/browser": "^5.0.0-beta1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0"
  }
}
